   					/* QStatusBar */
   	
 QStatusBar {
      border-top: 1px groove rgb(60, 75, 63);
      padding: 5px 10px;
 }

 QStatusBar::item {
     border-width: 0px
 }
 
 QStatusBar QLabel {
     border: 3px groove rgb(80, 79, 75);
     border-radius: 10px;
 }
   					
   					/* QStatusBar */

QCheckBox {
	background-color: transparent;
}

QRadioBox {
	background-color: transparent;
}	

QSpinBox {
	background-color: transparent;
    padding-right: 15px;
    border-radius: 3px;
	border: 1px outset rgb(80, 79, 75);
}
   			
   			
[isTitle=true] { 
	font-weight: bold; 
	font-size: 25pt; 
	padding: 0px 10px;
} 

[hasError=true] {
     color: rgb(60, 59, 55);
     background-color: rgb(200, 55, 43);
}

[greenText=true] {
     color: rgb(40, 200, 43);
}

[redText=true] {
     color: rgb(200, 55, 43);
}
